{
  "schemaVersion": 1,
  "id": "galacticraft-rewoven",
  "version": "$version",
  "name": "Galacticraft: Rewoven",
  "icon": "assets/galacticraft-rewoven/icon.png",
  "description": "FabricMC port of Galacticraft, written from the ground up by a new team.",
  "licence": "MIT",
  "contact": {
    "sources": "https://github.com/StellarHorizons/Galacticraft-Rewoven",
    "homepage": "https://hrznstudio.com",
    "issues": "https://github.com/StellarHorizons/Galacticraft-Rewoven/issues"
  },
  "authors": [
    {
      "name": "Horizon Studio",
      "contact": {
        "homepage": "https://hrznstudio.com",
        "discord": "https://discord.gg/8eVjQrb"
      }
